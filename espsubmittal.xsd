	<!-- 
		Report Responses  
	-->
	<xs:element name="reportResponse">
        <xs:annotation>
        	<xs:documentation>This element is the root element of the response to a report, 
        		file or file details submittal.
        	</xs:documentation>
        </xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="responseCode" type="responseCode">
					<xs:annotation>
						<xs:documentation>
							The response code returned from the submittal. 0 is success,
							any non-zero number is an error code defined in the responseCode section.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="responseDescription" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							The description of the error code or empty if the response is success
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="reportId" type="xs:token">
					<xs:annotation>
						<xs:documentation>
							The report ID about which this submittal is related
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="fileId" type="xs:token">
					<xs:annotation>
						<xs:documentation>
							If a file was uploaded, the file ID of the file.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="hash" type="xs:token">
					<xs:annotation>
						<xs:documentation>
							If a file was uploaded, the md5 hash of the file.
						</xs:documentation>
					</xs:annotation>
				</xs:element> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<!--
		file upload - This is better handled using a standard POST.
		There are three variables:
			reportId - the report this file is related to
			uploadFile - the file being uploaded 
	 -->
	 
	 
	<!--
		file details - Once a file is uploaded, details of the file may follow  
	-->
	<xs:element name="fileDetails">
		<xs:annotation>
			<xs:documentation>The root element of the file details.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reportId" type="xs:token">
					<xs:annotation>
						<xs:documentation>The report this file is related to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fileId" type="xs:token">
					<xs:annotation>
						<xs:documentation>the file this report is related to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fileName" type="xs:string">
					<xs:annotation>
						<xs:documentation>The name of the file being uploaded.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="locationOfFile" type="xs:anyURI">
					<xs:annotation>
						<xs:documentation>the URL where reported file was originally located</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="ipCaptureEvent" type="ipCaptureEvent">
					<xs:annotation>
						<xs:documentation>This represents the upload IP address of the file
						and when it occurred.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" maxOccurs="unbounded" ref="details"/>
				<xs:element minOccurs="0" ref="additionalInfo">
					<xs:annotation>
						<xs:documentation>Any additional information about the file (i.e., 
						associated comments, captions, etc.)
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="details">
        <xs:annotation>
        	<xs:documentation>All the file meta data name value pairs.</xs:documentation>
        </xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="nameValuePair" minOccurs="1" maxOccurs="unbounded" type="nameValue" >
					<xs:annotation>
						<xs:documentation>The list of meta data</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="type" type="metadataTypes">
				<xs:annotation>
					<xs:documentation>The type of meta data being sent</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="metadataTypes">
	    <xs:restriction base="xs:string">
			<xs:enumeration value="EXIF" />
			<xs:enumeration value="HASH" />
    	</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="responseCode">
        <xs:annotation>
        	<xs:documentation>For every submittal, there is a response code</xs:documentation>
        </xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Success</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1000">
				<xs:annotation>
					<xs:documentation>Authentication Required</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1001">
				<xs:annotation>
					<xs:documentation>Malformed (xml) report submittal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1002">
				<xs:annotation>
					<xs:documentation>Malformed file details submittal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1003">
				<xs:annotation>
					<xs:documentation>Malformed report closure submittal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1004">
				<xs:annotation>
					<xs:documentation>User does not have permission to upload files to this report</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1005">
				<xs:annotation>
					<xs:documentation>User does not have permission to upload file details to this report</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1006">
				<xs:annotation>
					<xs:documentation>User does not have permission to close this report</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1007">
				<xs:annotation>
					<xs:documentation>User attempted to upload a file to a report that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1008">
				<xs:annotation>
					<xs:documentation>User attempted to upload file details to a report that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1009">
				<xs:annotation>
					<xs:documentation>User attempted to close a report that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1010">
				<xs:annotation>
					<xs:documentation>Server error, report submittal failed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1011">
				<xs:annotation>
					<xs:documentation>Server error, file upload failed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1012">
				<xs:annotation>
					<xs:documentation>Server error, file details upload failed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1013">
				<xs:annotation>
					<xs:documentation>Server error, report closure failed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1014">
				<xs:annotation>
					<xs:documentation>Unable to upload file. File size too large</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1015">
				<xs:annotation>
					<xs:documentation>User attempted to upload file details to a file that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1016">
				<xs:annotation>
					<xs:documentation>Report Retracted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1017">
				<xs:annotation>
					<xs:documentation>User does not have permission to perform this action</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1018">
				<xs:annotation>
					<xs:documentation>Report specifies a previous report that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1019">
				<xs:annotation>
					<xs:documentation>User does not have permission to retract his report</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1020">
				<xs:annotation>
					<xs:documentation>User attempted to retract a report that does not exist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1021">
				<xs:annotation>
					<xs:documentation>User attempted to retract a report that is already closed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1022">
				<xs:annotation>
					<xs:documentation>User attempted to retract a report that is already retracted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="nameValue">
		<xs:sequence>
			<xs:element name="name" type="xs:string" >
				<xs:annotation>
					<xs:documentation>The name of the meta data variable.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="xs:string" >
				<xs:annotation>
					<xs:documentation>The value of the meta data variable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<!-- The final response from the server  -->
	<xs:element name="reportDoneResponse">
		<xs:annotation>
			<xs:documentation>The final response from the server once the report has been submitted, 
				all files and file details have been uploaded and the report done has been submitted.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="responseCode" type="responseCode">
					<xs:annotation>
						<xs:documentation>
							The response code returned from the submittal. 0 is success,
							any non-zero number is an error code defined in the responseCode section.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="reportId" type="xs:string"/>
				<xs:element name="files">
					<xs:annotation>
						<xs:documentation>The list of files successfully uploaded</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element minOccurs="0" maxOccurs="unbounded" name="fileId" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
