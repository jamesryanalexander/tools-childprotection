<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">
    <xs:annotation>
    	<xs:documentation>This document describes the ESP incident reporting format.</xs:documentation>
    </xs:annotation>
    <xs:element name="report">
		<xs:annotation>
			<xs:documentation>This is the main top level tag to define an incident report</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="incidentSummary">
					<xs:annotation>
						<xs:documentation>This contains general incident information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element maxOccurs="unbounded" ref="internetDetails" >
					<xs:annotation>
						<xs:documentation>This record summarizes the details of the incident being reported. 
							There can be more than one for different
							incident details, but there must be at least of these
							for each report.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="lawEnforcement" >
					<xs:annotation>
						<xs:documentation>The law enforcement contact information related to the event. This is used 
							if the incident information has already been reported to law enforcement or if the 
							request originated from law enforcement.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="reporter" >
					<xs:annotation>
						<xs:documentation>This is information related to the person or organization reporting the incident</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" maxOccurs="1" ref="personOrUserReported" >
					<xs:annotation>
						<xs:documentation>There can be 0 or 1 reported user or person involved in the incident</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" maxOccurs="unbounded" ref="victim" >
					<xs:annotation>
						<xs:documentation>There can be 0 or more victim records for each incident</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="additionalInfo">
					<xs:annotation>
						<xs:documentation>Any additional notes on this incident not covered by any other section</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="incidentSummary">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="incidentType" type="incidentType">
					<xs:annotation>
						<xs:documentation>
							This field describes the type of incident being reported. It&apos;s
							an enumerated value with certain predefined values.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="escalateToHighPriority" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							Company indicated a higher level of urgency (immediate risk to child) and the
							reason for this escalation
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="incidentDateTime" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>
							This required field will be populated with the date/time/time zone of the reported incident
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="incidentDateTimeDescription" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							This is a text field that allows you to enter any additional information 
							related to the incidentDateTime.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="internetDetails">
		<xs:annotation>
			<xs:documentation>
				These records summarize a specific instance to be reported. There can be
				several different types to reflect
				different instances. Sometimes, an incident report can contain many
				different, but related events such as a URL,
				email or chat. There can be 0 or more of the following subtypes.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="webPageIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred on a web page. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="emailIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred over Email. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="newsgroupIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred in a newsgroup.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="chatImIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred over IM or during a chat session</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="onlineGamingIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred during an online game.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="cellPhoneIncident" >
					<xs:annotation>
						<xs:documentation>This category is used when the primary method of communication between 
							the child and/or reported person or user is through a cell phone. This can include phone conversations, 
							text messages, or images sent through the phone.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="nonInternetIncident" >
					<xs:annotation>
						<xs:documentation>This category of the incident does not involve an online or computer component 
						or a cell phone (for example, face-to-face contact or potentially illegal hard copy or
						printed materials).
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="peer2peerIncident" >
					<xs:annotation>
						<xs:documentation>The incident occurred via a Peer to Peer (also known as P2P) decentralized network that 
						allows users to share files through a direct connection.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="webPageIncident">
		<xs:annotation>
			<xs:documentation>The incident occurred on a web page.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="url" maxOccurs="unbounded" type="xs:anyURI">
					<xs:annotation>
						<xs:documentation>
							This is the actual URL where the reported incident occurred. There
							can be as many of these as needed.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							This is a text field that allows you to enter
							any additional information related to the web page event.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute default="false" name="thirdPartyHostedContent" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Reported URL is hosted on another company&apos;s server. The 
					reporting company has no further information on this incident and is providing 
					it to NCMEC for informational purposes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="emailIncident">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="content" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The headers, subject and content of the communication</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Any additional notes to add to the email event</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="newsgroupIncident">
		<xs:complexType>
            <xs:annotation>
            	<xs:documentation>The incident occurred in a newsgroup.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
				<xs:element name="name" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The newsgroup name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="content" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>The content of the newsgroup posting including any headers, subject.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Any additional notes to add to the newsgroup event.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="chatImIncident">
		<xs:annotation>
			<xs:documentation>
				The incident occurred over instant messenger or during a chat session
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="chatClient" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The chat or instant messanger program used</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="chatRoomName" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The name of the chat room</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="content" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The content of the chat, any chat logs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Any additional information related to the chat/instant messaging incident
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="onlineGamingIncident">
		<xs:annotation>
			<xs:documentation>The incident occurred during an online game.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="gameName" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The name of the game the incident occurred in.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="console" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The name of the gaming console if one was used.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="content" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>The content of the message/chat that occurred during the game</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Any additional information about the online gaming incident</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="cellPhoneIncident">
		<xs:annotation>
			<xs:documentation>
				This incident occurred via cell phone, SMS technology or mobile app. This can include phone
				conversations, text messages, or images sent through the phone.
        	</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="phoneNumber" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							The cell phone number associated with the incident
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="latitude" minOccurs="0" type="latlong">
					<xs:annotation>
						<xs:documentation>
							Please provide the latitude associated with the incident.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="longitude" minOccurs="0" type="latlong">
					<xs:annotation>
						<xs:documentation>Please provide the longitude associated with the incident.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Any additional information about the cell phone incident.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="nonInternetIncident">
		<xs:annotation>
			<xs:documentation>
				This category should be used if the reported incident did not occur on the World Wide 
				Web, email, newsgroups, chat/instant message, online gaming, cell phone, SMS or peer to peer.
        	</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="locationName" minOccurs="1" type="xs:string" >
					<xs:annotation>
						<xs:documentation>
							The location associated with the incident
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="incidentAddress" minOccurs="0" maxOccurs="unbounded" type="address" >
					<xs:annotation>
						<xs:documentation>Incident&apos;s address</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo">
					<xs:annotation>
						<xs:documentation>Please describe in detail the incident being reported.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="peer2peerIncident">
		<xs:annotation>
			<xs:documentation>
				The incident occurred via a Peer to Peer (also known as P2P) decentralized network that allows 
				users to share files through a direct connection.
        	</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="client" minOccurs="1" type="xs:string" >
					<xs:annotation>
						<xs:documentation>
							The Peer to Peer Client used (i.e., Phex, Emule)
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ipCaptureEvent" type="ipCaptureEvent" minOccurs="0" maxOccurs="unbounded" >
					<xs:annotation>
						<xs:documentation>An IP address used by the person or user reported</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fileNames" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The name of file/s</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Any additional information about the cell phone incident.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lawEnforcement">
        <xs:annotation>
        	<xs:documentation>The law enforcement contact information related to the event. This is used 
					if the incident information has already been reported to law enforcement or if the 
					request originated from law enforcement.
        	</xs:documentation>
        </xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="agencyName" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The law enforcement agency name involved</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="caseNumber" minOccurs="0" type="xs:NMTOKEN" >
					<xs:annotation>
						<xs:documentation>The case number if known</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="officerContact" minOccurs="0" type="person" >
					<xs:annotation>
						<xs:documentation>The contact information for the investigator or officer on the case.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="reportedToLe" minOccurs="0" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Set this to true if the incident was reported to law enforcement by the company</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="servedLegalProcessDomestic" minOccurs="0" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Set this to true if domestic law enforcement has served legal process about this person/user or reported incident</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="servedLegalProcessInternational" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Set this to true if international law enforcement has served legal process about this person/user or reported incident</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:boolean">
								<xs:attribute name="fleaCountry" type="country">
									<xs:annotation>
										<xs:documentation>The name of the country associated with this report from which legal process was received.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reporter">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reportingPerson" type="person" >
					<xs:annotation>
						<xs:documentation>Details of the person reporting the information and the company&apos;s address</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="contactPerson" type="person" >
					<xs:annotation>
						<xs:documentation>
							Use this field if the company prefers law enforcement to contact another individual
							other than the person who submitted the report.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="companyTemplate" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>This open-text field allows a company to include language (not specific to the reported incident) 
						that they want included in every CyberTipline report. For instance, basic company information, how legal process 
						should be served, length of data retention, 24-hour emergency phone numbers, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="legalURL" minOccurs="0" type="xs:anyURI">
					<xs:annotation>
						<xs:documentation>Company can include a URL if they wish to direct law enforcement to a web page for further information.</xs:documentation>
					</xs:annotation>
				</xs:element> 
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="personOrUserReported">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="personOrUserReportedPerson" minOccurs="0" type="person" >
					<xs:annotation>
						<xs:documentation>The person information about the reported person or user in the incident.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="vehicleDescription" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The description for the vehicle of the reported person.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="espIdentifier" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The unique ID of the reported person or user in the ESP&apos;s system (i.e., friend ID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="screenName" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The screen name of the reported person or user</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="profileUrl" minOccurs="0" maxOccurs="unbounded" type="xs:anyURI" >
					<xs:annotation>
						<xs:documentation>The different identifying URLs for this reported person or user</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ipCaptureEvent" type="ipCaptureEvent" minOccurs="0" maxOccurs="unbounded" >
					<xs:annotation>
						<xs:documentation>An originating IP address used by the reported person or user and when it was used </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="thirdPartyUserReported" minOccurs="0" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Reported person or user is using another company&apos;s service. The reporting company has no further 
						information about this person or user.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="priorCTReports" minOccurs="0" maxOccurs="unbounded" type="xs:int">
					<xs:annotation>
						<xs:documentation>List report ID numbers for any prior CyberTipLine reports on this reported person or user</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="groupIdentifier" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>The ESP believes the reported person or user is engaged in an organized online group related to child sexual exploitation.
						This open text field is used  to provide unique group identifiers (i.e., group name, group ID, etc.). </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="additionalInfo" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Any additional notes on this reported person or user not covered by any other section</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="victim">
        <xs:annotation>
        	<xs:documentation>There can be 0 or more victim records for each incident</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="victimPerson" type="person" >
					<xs:annotation>
						<xs:documentation>The victim&apos;s personal details such as name, address, phone numbers.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="espIdentifier" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The unique ID of the victim in the ESP&apos;s system (i.e., friend ID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="screenName" minOccurs="0" type="xs:string" >
					<xs:annotation>
						<xs:documentation>The screen name of the victim</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="profileUrl" minOccurs="0" maxOccurs="unbounded" type="xs:anyURI" >
					<xs:annotation>
						<xs:documentation>The identifying URL(s) for the child victim</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ipCaptureEvent" type="ipCaptureEvent" minOccurs="0" maxOccurs="unbounded" >
					<xs:annotation>
						<xs:documentation>An originating IP address used by the victim and when it was used</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="schoolName" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>Child victim&apos;s school name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="priorCTReports" minOccurs="0" maxOccurs="unbounded" type="xs:int">
					<xs:annotation>
						<xs:documentation>List report ID numbers for any prior CyberTipLine reports on this child victim</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="additionalInfo" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>Any additional notes on this child victim</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="additionalInfo" type="xs:string" >
		<xs:annotation>
			<xs:documentation>Any additional notes on this incident not covered by any other section</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="content" type="xs:string" >
		<xs:annotation>
			<xs:documentation>Text content of an internet incident such as email headers </xs:documentation>
		</xs:annotation>
	</xs:element>

	<!-- Complex or reused data types -->
	<xs:complexType name="ipCaptureEvent">
        <xs:annotation>
        	<xs:documentation>This represents the origination IP address of an action and when it occurred. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="ipAddress" type="xs:string" >
				<xs:annotation>
					<xs:documentation>The IP address from which the incident occurred</xs:documentation>
				</xs:annotation>
			</xs:element>
